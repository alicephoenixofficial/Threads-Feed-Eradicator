{
  "manifest_version": 3,
  "name": "Threads Feed Eradicator",
  "version": "1.0",
  "description": "This extension hides the Feed button from the profile page on Threads.",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
      "https://www.threads.net/",  
      "https://www.threads.net/*"

      ]
    }
  ]
}
